IssuesController#index
<h1>List of Issues</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Date Started</th>
  </tr>

  <% @issues.each do |issue| %>
    <tr>
      <td>
        <%= issue.issue_name %>
      </td>
      <td>
        <%= issue.date_started %>
      </td>
      <td>
        <%= link_to 'Show', issue %>
        <%= link_to "Edit Issue", edit_issue_path(issue)%>
        <%= link_to 'Delete Issue', issue, method: :delete, data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
    <% end %>
</table>
<!-- I need a button here that will allow the user to add a new issue. -->
<%= link_to 'New Issue', new_issue_path %>
