<!DOCTYPE html>
<html>
  <head>
    <title>EhmTrack</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <header>
  </header>
  <body>
    <div class="card-content green-text"><%= notice %></div>
    <div class="card-content red-text"><%= alert %></div>
   
    <%= render 'layouts/header' %>
    
    <main>
      <div class="container">
        <%= yield %>
      </div>
    </main>
    
    <%= render 'layouts/footer' %>
  </body>
</html>
